{
  "name": "chatfuel",
  "author": {
    "name": "niktverd"
  },
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start:server": "vite-node ./server/index.ts",
    "start:client": "vite",
    "build": "tsc -b && vite build",
    "lint": "run-s lint:js lint:styles lint:prettier typecheck",
    "lint:fix": "run-s lint:js:fix lint:styles:fix lint:prettier:fix typecheck",
    "lint:js": "eslint '**/*.{js,jsx,ts,tsx}' --max-warnings=0",
    "lint:js:fix": "npm run lint:js -- --quiet --fix",
    "lint:styles": "stylelint **/*.css",
    "lint:styles:fix": "stylelint **/*.css --fix",
    "lint:prettier": "prettier --check '**/*.{js,jsx,ts,tsx,css,json,yaml,yml,md}'",
    "lint:prettier:fix": "prettier --write '**/*.{js,jsx,ts,tsx,css,json,yaml,yml,md}'",
    "typecheck": "tsc -p . --noEmit",
    "preview": "vite preview"
  },
  "dependencies": {
    "@apollo/client": "^3.12.4",
    "@apollo/server": "^4.11.2",
    "@graphql-tools/schema": "^10.0.14",
    "@types/cors": "^2.8.17",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "graphql": "^16.10.0",
    "graphql-subscriptions": "^3.0.0",
    "graphql-ws": "^5.16.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-virtuoso": "^4.12.3",
    "vite-node": "^2.1.8",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@gravity-ui/prettier-config": "^1.1.0",
    "@gravity-ui/stylelint-config": "^5.0.0",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5"
  }
}
